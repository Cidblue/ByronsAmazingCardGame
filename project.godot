; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "PanelContainer",
"class": "CardPanel",
"language": "GDScript",
"path": "res://scripts/CardPanel.gd"
} ]
_global_script_class_icons={
"CardPanel": ""
}

[application]

config/name="ByronsAmazingCG"
config/description="A Card Matching Memory Game"
run/main_scene="res://SceneSwitcher.tscn"
boot_splash/image="res://assets/BACGLogo.png"
config/icon="res://assets/BACGLogo.png"

[compression]

formats/zstd/compression_level=10
formats/zlib/compression_level=9
formats/gzip/compression_level=9

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
